package java1;

import java.util.Scanner;

public class odd {

	public static void main(String[] args) {
		int n;
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the number:");
		n=scan.nextInt();
		if(n%2==0) {
			System.out.println("even");
		}
		else
		{
			System.out.println("odd");
		}
	}

}
